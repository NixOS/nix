let
  fromEnv = var: def:
    let val = builtins.getEnv var; in
    if val != "" then val else def;
in {
  perl = "@perl@";
  shell = "@bash@";
  coreutils = "@coreutils@";
  bzip2 = "@bzip2@";
  gzip = "@gzip@";
  xz = "@xz@";
  tar = "@tar@";
  tarFlags = "@tarFlags@";
  tr = "@tr@";
  curl = "@curl@";
  nixBinDir = fromEnv "NIX_BIN_DIR" "@bindir@";
}
